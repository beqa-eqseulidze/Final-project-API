{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "float",
      "required": true
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "warranty": {
      "type": "string"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::two-level-sub-category.two-level-sub-category",
      "inversedBy": "products"
    }
  }
}
